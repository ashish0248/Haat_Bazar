<!-- レイアウトを崩さないように3個ずつ区切る -->
<% count = 0 %>
<% tag_list.each do |tag| %>
	<% if count == 3 %>
		<br>
		<% count = 0 %>
	<% end %>
	<%= link_to homes_path(tag_name: tag), style:"text-decoration: none;" do %> 
   		<span class="label label-primary"><%= tag %></span>
   	<% end %>
   	<% count = count + 1 %>
<% end %>