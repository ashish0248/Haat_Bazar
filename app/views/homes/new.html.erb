
<!-- お問い合わせフォーム -->
<div class="body">
    <div class="container">
      <div class="row">
        <h3>お問い合わせ</h3>
        <div>
          <%= form_for(@contact_new, url: homes_path) do |f| %>
            <%= f.label :お名前 %>
            <%= f.text_field :name %><br>
            <%= f.label :Eメールアドレス %>
            <%= f.text_field :email %><br>
            <%= f.label :お問い合わせ内容 %>
            <%= f.text_area :message %>
            <%= f.submit '送信' %>
            <% end %>
        </div>
      </div>
    </div>
</div>
